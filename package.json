{
  "name": "incident-reporter",
  "version": "1.0.0",
  "description": "Emergency incident reporting system with photo verification",
  "main": "api/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon api/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm install && npm run build",
    "start": "node api/index.js",
    "vercel-build": "cd client && npm install && npm run build"
  },
  "keywords": ["emergency", "incident", "reporting", "ambulance"],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mongoose": "^8.0.3",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
